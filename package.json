    "@vercel/analytics": "^1.0.0",
    "autoprefixer": "10.4.14",
    "classnames": "2.3.2",
    "esbuild": "0.17.17",
    "github-slugger": "2.0.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.2",
    "mdx-bundler": "9.2.1",
    "next": "13.4.10",
    "next-themes": "0.2.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-citation": "1.0.1",
