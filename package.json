  "devDependencies": {
    "@next/bundle-analyzer": "13.4.10",
    "@svgr/webpack": "7.0.0",
    "cross-env": "7.0.3",
    "dedent": "0.7.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "globby": "13.1.4",
    "husky": "8.0.3",
    "inquirer": "9.1.5",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.7",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1"
